{
  "name": "webmidi-controller",
  "version": "0.1.2",
  "description": "webmidi controller",
  "files": [
    "src/"
  ],
  "scripts": {
    "dev": "npm run serve && webpack --watch",
    "serve": "webpack serve",
    "build": "npm run clean && cross-env NODE_ENV=production webpack",
    "clean": "rm -rf ./dist ./.cache",
    "deploy": "npm run build && gh-pages -d dist",
    "push": "git add --all && git commit --amend --no-edit && git push origin primary --force && npm run deploy"
  },
  "keywords": [
    "webmidi",
    "midi"
  ],
  "author": "Ken Frederick",
  "license": "MIT",
  "devDependencies": {
    "@polymer/iron-icons": "^3.0.1",
    "@types/node": "^14.14.14",
    "@types/web-bluetooth": "0.0.9",
    "@types/webmidi": "^2.0.4",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-env": "^1.16.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^5.0.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^1.2.0",
    "lit-element-scss-loader": "git+https://github.com/clicksolutions/lit-element-scss-loader.git",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "nunjucks": "^3.2.2",
    "nunjucks-webpack-plugin": "^5.0.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^2.0.0",
    "to-string-loader": "^1.1.6",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-loader": "^3.5.4",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lit-element": "^2.4.0"
  }
}
